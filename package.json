{
	"name": "ach-truhlarstvi-wordpress-theme",
	"description": "Wordpress theme for carpenter and wood-mage Adam Ach Hubner",
	"repository": "https://github.com/karelsuchomel/ach-truhlarstvi",
	"author": "Karel Suchomel",
	"license": "MIT",
	"private": true,
	"dependencies": {
		"autoprefixer": "^9.7.3",
		"bootstrap-scss": "^4.4.1",
		"ftp-deploy": "^2.3.3",
		"postcss-cli": "^6.1.3",
		"promise-ftp-common": "^1.1.5",
		"focus-visible": "^5.0.2"
	},
	"devDependencies": {
		"@babel/core": "^7.2.2",
		"@babel/preset-env": "^7.2.3",
		"@babel/preset-react": "^7.9.4",
		"rollup": "^1.0.0",
		"rollup-plugin-babel": "^4.2.0",
		"rollup-plugin-commonjs": "^9.2.0",
		"rollup-plugin-node-resolve": "^4.0.0",
		"rollup-plugin-uglify": "^6.0.4"
	},
	"scripts": {
		"scss": "dart ~/dart-sass/bin/sass.dart --watch --style expanded src/scss/index.scss:build/css/bundle.css",
		"autoprefixer": "postcss build/css/bundle.css -u autoprefixer --autoprefixer.browsers '&gt; 5%, ie 11' -o build/css/bundle.css --watch",
		"start": "yarn run scss && yarn run autoprefixer",
		"deploy": "node scripts/deploy.js",
		"dev": "rollup -c -w"
	},
	"resolutions": {
		"@babel/preset-env": "7.5.5"
	}
}
